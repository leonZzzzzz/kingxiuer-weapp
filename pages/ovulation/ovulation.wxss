  margin-top: 10rpx;
  font-size: 28rpx;
}

.explainBtn .yuejing,
.explainBtn .pailuan,
.explainBtn .pailuanDay {
  /* font-size: 12pt; */
  display: flex;
  line-height: 11pt;
}

.explainBtn .yuejing .yuejingIcon,
.explainBtn .pailuan .pailuanIcon,
.explainBtn .pailuanDay .pailuanDayIcon {
  /* font-size: 12pt; */
  height: 11pt;
  width: 11pt;
