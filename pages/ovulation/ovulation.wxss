.scroll-photo .scroll-photo_header {
  padding: 30rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.scroll-photo .scroll-photo_header .scroll-photo_year {
  padding: 2rpx 0;
  padding-right: 12rpx;
  font-size: 24rpx;
  color: #fff;
  line-height: 1;
  background: #ff8b8b;
  border-radius: 0 8px 8px 0;
}

.scroll-photo .scroll-photo_header .scroll-photo_intro {
  font-size: 28rpx;
  padding: 6rpx 40rpx;
  line-height: 1;
  /* background: #ff8b8b; */
  border: 1px solid #ff8b8b;
  border-radius: 20px;
  color: #ff8b8b;
}

.scroll-photo .scroll-photo_content {
  height: 460rpx;
}

.scroll-photo .scroll-photo_content .scroll-photo_list {
  position: relative;
  border-bottom: 1px solid #fff;
}

.scroll-photo .scroll-photo_content .scroll-photo_list .content {
  transform: translateX(-120rpx);
  transition: all 1s;
}

.scroll-photo .scroll-photo_content .scroll-photo_list .del {
  position: absolute;
  right: 0;
  top: 0;
  background-color: orangered;
  width: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  height: 40px;
  transform: translateX(120rpx);
  transition: all 0.1s;
}

.touch-move-active .content {
  transform: translateX(0rpx) !important;
  transition: all 1s !important;
}

