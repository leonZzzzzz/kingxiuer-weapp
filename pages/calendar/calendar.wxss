.calendar {
  line-height: 1;
  background:#fff;
  margin-bottom:20rpx;
}
.calendar .week {
  padding: 14rpx 0rpx;
  display: flex;
  justify-content: space-around;
  font-size: 26rpx;
  color: #a98457;
  border-bottom: 1px solid #f0eee8;
}
.calendar .day {
  display: flex;
  flex-wrap: wrap;
}

/* 大盒子 */
.calendar .day .item {
  position: relative;
  flex: 106rpx 0;
  height: 106rpx;
  font-size: 26rpx;
  text-align: center;
  border-bottom: 1px solid #f0eee8;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  /* flex-direction: column; */
}

.calendar .day .item.border::after {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 60rpx;
  height: 60rpx;
  border: 1px solid #e97aa6;
  border-radius: 50%;
}

.calendar .day .item::before {
  content: "";
  height: 36rpx;
  /* width: 36rpx; */
  display: block;
}

.calendar .day .item .list {
  height: 34rpx;
  line-height: 34rpx;
  color: #666;
}



.calendar .day .item .list.menses-color {
  color: #fff;
  /* border: 1px solid black; */
  background-color: #fc9cc0;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  line-height: 56rpx;
}

.calendar .day .item .list.ovluate {
  width: 56rpx;
  height: 56rpx;
  position: relative;
  color: #fff;
  background-color: #aedb88;
  border-radius: 50%;
  line-height: 56rpx;
  /* border: 1px solid black; */
}

.calendar .day .item .list.ovluate .ovluateDay {
  position: absolute;
  top: 0;
  left: 0;
  width: 56rpx;
  height: 56rpx;
  font-size: 22rpx;
  text-align: center;
  line-height: 56rpx;
  color: #fff;
  background: #5bac17;
  border-radius: 50%;
}
/* .calendar .day .item .list.menses {
  background: #ffcbe8;
  color: #fff;
 
} */
.calendar .day .item .list.menses,
.calendar .day .item .list.start,
.calendar .day .item .list.end{
  /* border-radius: 20rpx 0 0 20rpx; */
  color: #fff;
  background-color: #fc9cc0;
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  line-height: 56rpx;
  /* background-color: #66fcff; */
}

/* .calendar .day .item .list.end {
  border-radius: 0 20rpx 20rpx 0;
} */

.calendar .day .item .list:last-child .icon {
  font-size: 20rpx;
  color: #ec4a85;
  display: block;
  margin-left: 5rpx;
  position: absolute;
  left: 48%;
  transform: translate(-50%,0);
  bottom:-6rpx;
}

.calendar .tip {
  display: flex;
