page{
  background:rgb(240, 239, 239)
}
.nav{
  display: flex;
background-color: #fff;
font-size: 30rpx;
padding:20rpx 30rpx;
}
.nav_img{
  width:345rpx;
  text-align: center;
}
.nav_img>text{
}
.active{
  border-bottom:2px solid #FAA4A3;
  padding-bottom:10rpx;
}
.content{
  margin:30rpx;
  margin-top: 110rpx;
}
.box{
  background: #fff;
margin-bottom: 30rpx;
border-radius: 15rpx;
}
.box>image{
  width:690rpx;height:260rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}
.title{
  padding:20rpx;
  display: flex;
justify-content: space-between;
}
.title>view{
  width:450rpx;
}
.title>text{
  font-size:26rpx;
  color:#999;
}
.result{
  background:#FAA4A3;
  color:#fff !important;
  font-size:26rpx;
  padding:15rpx 25rpx;
  border-radius: 40rpx;
  height:38rpx;
  line-height: 38rpx;
}
.tip{
  padding: 0 20rpx 20rpx 20rpx;
font-size: 28rpx;
color: #999;
}
.btn{
  padding: 20rpx;
  display: flex;
  justify-content: center;
}
.btn>text{
  background:#FAA4A3;
  color:#fff;
  font-size:28rpx;
  padding:10rpx 0;
  border-radius: 50rpx;
  width:260rpx;
  text-align: center;
  margin:0 30rpx;
}