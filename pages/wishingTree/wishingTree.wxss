/* pages/wishingTree/wishingTree.wxss */
page{
  height: 100%;
  width: 100%;
  position: relative;
}
.bg{
  width: 100%;
  height: 100%;
}
.bg > image {
  width: 100% ;
  height: 100% ;
}
.newbtn{
  position: fixed;
bottom: 50rpx;
display: flex;
width: 100%;
}
.newbtn>button{
  padding: 0;
  background-color: #fcdbda;
  color: #712726;
  width: 300rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 90rpx;
  font-weight: bold;
  box-shadow: 5px 5px 10px #dc8686;
  /* position: fixed; */
  /* bottom: 100rpx; */
  /* left: 50%; */
  /* transform: translate(-50%); */

}
.wishingContent {
  width: 50rpx;
  height: 130rpx;
  left: 150rpx;
  top: 500rpx;
  z-index: 1;
  position:absolute;
}
.wishingContent .haoyunImg{
  width: 50rpx;
  height: 130rpx;
  left: 150rpx;
  top: 500rpx;
  z-index: 1;
  position: fixed;
}
.move .haoyunImg{
  width: 50rpx;
  height: 130rpx;
  left: 150rpx;
  top: 500rpx;
  z-index: 1;
  position: absolute;
  transition:3s all ;

}
.btnList{

  position:fixed;
  right: 0rpx;
  top: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.btnList view{
  line-height: 28rpx;
  height: 28rpx;
  /* he */
}

.btnList .huanyuan,
.btnList .zhenduan{
  padding: 0;
  line-height: 80rpx;
  /* padding:20rpx; */
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  color: #fff;
  margin: 20rpx;
  font-size: 22rpx;
  /* font-weight: bold; */
  background-color: #ff9e9e;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;

}

